    "cross-env": "^5.2.0",
    "nuxt": "^2.4.0",
    "vue-clipboard2": "^0.3.0",
    "vue-jsonp": "^0.1.8",
    "vue-scrollto": "^2.14.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": ">=2.14.0",
    "eslint-plugin-jest": ">=21.24.1",
    "eslint-plugin-node": ">=7.0.1",
